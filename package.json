{
  "name": "alehub_api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:swc": "swc --ignore \"**/*.spec.ts,src/hmr.ts\" --delete-dir-on-start --out-dir dist/ src/",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "prebuild": "rm -rf dist",
    "seeder:up": "npx sequelize-cli db:seed:all",
    "seeder:undo": "npx sequelize-cli db:seed:undo",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run prebuild && npm run build --prod && node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.5",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/platform-socket.io": "^10.2.4",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/websockets": "^10.2.4",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "googleapis": "^129.0.0",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.5.2",
    "nodemailer": "^6.9.4",
    "path": "^0.12.7",
    "rebuild": "^0.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "trim-request": "^1.0.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.81",
    "@types/babel__core": "^7.20.1",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.197",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.9",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "babel-register": "^6.26.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-check-file": "^2.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "_moduleAliases": {
    "@enterprise": "./dist/v1/enterprise/*",
    "@root": "./dist/*",
    "@shared": "./dist/shared/*",
    "@decorators": "./dist/shared/decorators/*",
    "@enums": "./dist/shared/enums/*",
    "@fake-data": "./dist/shared/fake_data/*",
    "@filters": "./dist/shared/filters/*",
    "@guards": "./dist/shared/guards/*",
    "@mail-templates": "./dist/shared/mail-templates/*",
    "@middlewares": "./dist/shared/middlewares/*",
    "@models": "./dist/shared/models/*",
    "@repositories": "./dist/shared/repositories/*",
    "@services": "./dist/shared/services/*",
    "@utils": "./dist/shared/utils/*",
    "@auth": "./dist/v1/auth/*",
    "@user": "./dist/v1/user/*"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "yarn format",
      "git add ."
    ]
  }
}
